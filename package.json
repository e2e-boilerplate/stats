{
  "name": "stats",
  "version": "1.0.0",
  "description": "Gather traffic data for e2eBoilerplate org",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint . --ext .js --fix",
    "get:repos:list": "node -r esm src/get_repos_list.js",
    "get:redacted:list": "node -r esm src/get_redacted_list.js",
    "add:clone:stats": "node -r esm src/clone.js --token ${{ secrets.GITHUB_TOKEN }}",
    "add:referrers:stats": "node -r esm src/referrers.js",
    "add:views:stats": "node -r esm src/views.js",
    "stats": "node -r esm ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplate/stats.git"
  },
  "keywords": [
    "e2eboilerplate"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e2e-boilerplate/stats/issues"
  },
  "homepage": "https://github.com/e2e-boilerplate/stats#readme",
  "devDependencies": {
    "eslint": "7.6.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty --verbose"
    }
  },
  "lint-staged": {
    "./**/*.{js}": [
      "eslint . --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "esm": "3.2.25",
    "markdown-table": "2.0.0",
    "minimist": "1.2.5",
    "pino": "6.5.0",
    "pino-pretty": "4.1.0",
    "rimraf": "3.0.2"
  }
}
